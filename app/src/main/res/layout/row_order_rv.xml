<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/card_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#fff"
    app:cardElevation="@dimen/space_3"
    app:cardUseCompatPadding="true"
    card_view:cardCornerRadius="4dp">


    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/background"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <Button
                android:layout_width="120dp"
                android:id="@+id/order_picked"
                android:textColor="@color/white"
                android:text="Pick Order"
                android:visibility="gone"
                android:layout_marginTop="@dimen/space_10"
                android:layout_marginLeft="@dimen/space_10"
                android:background="@drawable/bg_rounded_button"
                android:layout_height="35dp" />


            <ImageView
                android:visibility="gone"
                android:layout_width="40dp"
                android:id="@+id/call"
                android:paddingLeft="@dimen/space_5"
                android:paddingRight="@dimen/space_5"
                android:paddingTop="@dimen/space_5"
                android:paddingBottom="@dimen/space_5"
                android:layout_marginTop="@dimen/space_10"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="30dp"
                android:src="@drawable/call"
                android:layout_height="40dp"
                tools:ignore="VectorDrawableCompat" />

            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/space_10"
                android:layout_below="@+id/call"
            android:id="@+id/re"
            android:layout_marginRight="@dimen/space_10"
            android:layout_marginTop="@dimen/space_10"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="2"
                android:weightSum="2">

                <LinearLayout
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginRight="1dp"
                    android:weightSum="2"
                    android:gravity="left|center">
                <com.godeliver.user.Fonts.LatoBLack
                    android:layout_width="65dp"
                    android:layout_height="wrap_content"
                    android:text="Order ID:"
                    android:textColor="@color/dark_gray"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold" />


                <com.godeliver.user.Fonts.LatoBLack
                    android:id="@+id/tv_order_no"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="ID"
                    android:textColor="#000"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="end|center">
                <com.godeliver.user.Fonts.LatoBLack
                    android:id="@+id/tv_order_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="end"
                    android:paddingBottom="@dimen/space_5"
                    android:textColor="@color/green"
                    android:textSize="16sp" />
            </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/space_2"
                android:orientation="horizontal"
                android:layout_weight="2"
                android:weightSum="2">

                <LinearLayout
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginRight="1dp"
                    android:weightSum="2"
                    android:gravity="left|center">
                <com.godeliver.user.Fonts.LatoBLack
                    android:layout_width="65dp"
                    android:layout_height="wrap_content"
                    android:text="Placed On:"
                    android:textColor="@color/dark_gray"
                    android:textSize="@dimen/text_size_small" />

                <com.godeliver.user.Fonts.LatoBLack
                    android:id="@+id/tv_order_date"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="Date"
                    android:textColor="#000"
                    android:textSize="@dimen/text_size_small" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:gravity="left|top">
                <com.godeliver.user.Fonts.LatoBLack
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="Time:"
                    android:gravity="center|left"
                    android:textColor="@color/dark_gray"
                    android:textSize="@dimen/text_size_small" />

                <com.godeliver.user.Fonts.LatoBLack
                    android:id="@+id/tv_order_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="time"
                    android:gravity="left|top"
                    android:layout_gravity="top"
                    android:textColor="#000" />
                </LinearLayout>
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/space_2"
                android:orientation="horizontal"
                android:layout_weight="2"
                android:weightSum="2">

                <LinearLayout
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginRight="1dp"
                    android:weightSum="2"
                    android:gravity="left|top">

                <com.godeliver.user.Fonts.LatoBLack
                    android:layout_width="65dp"
                    android:layout_height="wrap_content"
                    android:text="Receiver Name:"
                    android:textColor="@color/dark_gray"
                    android:textSize="@dimen/text_size_small" />

                <com.godeliver.user.Fonts.LatoBLack
                    android:id="@+id/tv_recivername"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:gravity="top|left"
                    android:layout_gravity="top|left"
                    android:textColor="#000"
                    android:textSize="@dimen/text_size_small" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:gravity="left|center">
                <com.godeliver.user.Fonts.LatoBLack
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="Total Items:"
                    android:textColor="@color/dark_gray"
                    android:textSize="@dimen/text_size_small" />

                <com.godeliver.user.Fonts.LatoBLack
                    android:id="@+id/tv_order_item"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|left"
                    android:gravity="left|top"
                    android:text="ArtÃ­culo total"
                    android:textColor="#000" /></LinearLayout>
            </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height=".5dp"
                    android:background="#DCDCDC"
                    android:layout_margin="5dp"></View>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/space_2"
                    android:orientation="horizontal"
                    android:layout_weight="2"
                    android:weightSum="2">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginRight="1dp"
                        android:weightSum="2"
                        android:gravity="left|top">
                        <com.godeliver.user.Fonts.LatoBLack
                            android:layout_width="65dp"
                            android:layout_height="wrap_content"
                            android:text="House No.:"
                            android:gravity="left|top"
                            android:textColor="@color/dark_gray"
                            android:textSize="@dimen/text_size_small" />

                        <com.godeliver.user.Fonts.LatoBLack
                            android:id="@+id/tv_house"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:layout_gravity="top"
                            android:gravity="left|top"
                            android:textColor="#000" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="2"
                        android:gravity="left|center">
                        <com.godeliver.user.Fonts.LatoBLack
                            android:layout_width="65dp"
                            android:layout_height="wrap_content"
                            android:visibility="gone"
                            android:text="Society Name"
                            android:textColor="@color/dark_gray"
                            android:textSize="@dimen/text_size_small" />

                        <com.godeliver.user.Fonts.LatoBLack
                            android:id="@+id/tv_societyname"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:visibility="gone"
                            android:textColor="#000"
                            android:textSize="@dimen/text_size_small" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/space_2"
                android:orientation="horizontal"
                android:layout_weight="2"
                android:weightSum="2">

                <LinearLayout
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginRight="1dp"
                    android:weightSum="2"
                    android:gravity="left|center">

    <com.godeliver.user.Fonts.LatoBLack

        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:textColor="@color/dark_gray"
        android:text="Store Name :  "
        android:textSize="@dimen/text_size_small" />


    <com.godeliver.user.Fonts.LatoBLack
        android:id="@+id/store_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_gravity="top|left"
        android:textSize="13sp"
        android:gravity="left|center"
        android:textColor="#000" />
                </LinearLayout>
</LinearLayout>

        </LinearLayout>


        </RelativeLayout>
        <View
